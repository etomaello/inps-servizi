{
  "name": "inps-github-pages",
  "version": "1.0.0",
  "description": "GitHub Pages site inspired by INPS (Istituto Nazionale Previdenza Sociale)",
  "private": true,
  "keywords": [
    "inps",
    "github-pages",
    "italian-design-system",
    "bootstrap"
  ],
  "license": "MIT",
  "author": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --open --config webpack.config.dev.js",
    "build": "npm run sass:build && webpack --config webpack.config.prod.js",
    "serve": "npx http-server -a localhost -p 6730 -o",
    "dev:worker": "npm run build && npx wrangler pages dev ./dist --port 8788",
    "sass:watch": "sass --watch scss:css --style compressed",
    "sass:build": "sass scss:css --style compressed"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.6.0",
    "sass": "^1.77.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0",
    "wrangler": "^4.60.0"
  }
}
